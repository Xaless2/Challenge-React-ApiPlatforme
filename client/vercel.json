{
    "version": 2,
    "builds": [
      {
        "src": "src/main.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/register",
        "dest": "/index.html"
      },
      {
        "src": "/login",
        "dest": "/index.html"
      },
      {
        "src": "/dashboard",
        "dest": "/index.html"
      },
      {
        "src": "/etablishment",
        "dest": "/index.html"
      },
      {
        "src": "/profile",
        "dest": "/index.html"
      },
      {
        "src": "/establishments/:id",
        "dest": "/index.html"
      },
      {
        "src": "/create-performance",
        "dest": "/index.html"
      },
      {
        "src": "/create-slot",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "github": {
      "productionBranch": "prod"
    }
  }
  